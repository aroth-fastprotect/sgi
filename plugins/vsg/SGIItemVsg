#pragma once
#include <sgi/plugins/SGIItemBase.h>
#include <sgi/plugins/SGIHostItemVsg.h>

namespace sgi {

enum SGIItemType_vsg
{
    SGIItemType_plugin_vsg = SGIItemTypePluginBaseVsg,
};

typedef SGIItemInfoSharedPtr<vsg::Object, vsg::ref_ptr<vsg::Object> > SGIItemInfoVsg;
typedef SGIHostItemImpl<SGIItemInfoVsg> SGIHostItemVsg;
typedef SGIItemHolderT<SGIItemInfoVsg> SGIItemHolderVsg;
typedef SGIItemT<SGIHostItemVsg, SGIItemHolderVsg> SGIItemVsg;

} // namespace sgi
